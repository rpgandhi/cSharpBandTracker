@{
Layout = "_Layout";
}

<h1>Band List</h1>
<hr />
<h2>Bands:</h2>
@if (Model.Count != 0)
{
  <ul>
    @foreach (var band in Model)
    {
      <li><a href="/bands/@band.GetBandId()">@band.GetBandName()</a></li>
    }
  </ul>
}
<h4><a href="/bands/new">Add a new band</a></h4>
<h4><a href="/">Home</a></h4>
